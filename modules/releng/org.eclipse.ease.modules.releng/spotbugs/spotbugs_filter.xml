<FindBugsFilter>

    <!-- Ignore generated code -->
    <Match>
        <Or>
        	<Class name="com.infineon.tools.configuration.IConfigurationFactory"/>
        	<Class name="com.infineon.tools.configuration.IConfigurationPackage"/>
            <Package name="com.infineon.tools.configuration.impl"/>
            <Package name="com.infineon.tools.configuration.util"/>
            <Package name="com.infineon.tools.hexparser.antlr"/>
        </Or>
    </Match>

    <!-- Ignore spotbugs reports from incomplete detectors -->
    <Match>
        <Bug pattern="TESTING"/>
    </Match>
</FindBugsFilter>