<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.core.contenttype.contentTypes">
      <content-type
            base-type="org.eclipse.core.runtime.text"
            default-charset="ISO-8859-1"
            file-extensions="scala"
            id="org.eclipse.ease.lang.scala.content-type"
            name="Scala Source File"
            priority="low">
      </content-type>
   </extension>
   <extension
         point="org.eclipse.ease.scriptType">
      <scriptType
            defaultExtension="java"
            headerParser="org.eclipse.ease.lang.jvm.compiled.JVMCompiledHeaderParser"
            name="Java">
         <binding
               contentType="javaSource">
         </binding>
         <binding
               contentType="org.eclipse.ease.lang.scala.content-type">
         </binding>
         <binding
               contentType="scala.tools.eclipse.scalaSource">
         </binding>
      </scriptType>
   </extension>
   <extension
         point="org.eclipse.ease.language">
      <engine
            class="org.eclipse.ease.lang.jvm.compiled.JVMCompiledScriptEngine"
            debugger="false"
            id="org.eclipse.ease.jvm.compiled"
            name="JVM"
            priority="20">
         <binding
               scriptType="Java">
         </binding>
      </engine>
   </extension>
</plugin>
