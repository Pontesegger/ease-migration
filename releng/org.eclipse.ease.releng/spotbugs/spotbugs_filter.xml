<FindBugsFilter>

    <!-- Ignore generated code -->
    <Match>
        <Or>
            <Package name="org.eclipse.ease.lang.unittest.definition.*"/>
            <Package name="org.eclipse.ease.lang.unittest.runtime.*"/>
        </Or>
    </Match>

    <!-- Ignore spotbugs reports from incomplete detectors -->
    <Match>
        <Bug pattern="TESTING"/>
    </Match>
</FindBugsFilter>